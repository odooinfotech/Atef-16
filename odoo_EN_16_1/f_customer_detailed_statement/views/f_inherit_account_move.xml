<odoo>


	<record id="f_ex_journal_entry_debt" model="ir.ui.view">
		<field name="name">f.journal.entry.ex.debt</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_form" />
		<field name="arch" type="xml">
			<xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='other_info']/group[1]/group[not(@name)][1]/field[@name='inalterable_hash']" position="after">

				<field name="f_exclude_entry" />
			</xpath>

			<xpath expr="//field[@name='partner_id']" position="after">
				<field name="f_partner_ref" attrs="{'invisible': [('move_type', '=', ('entry'))]}"/>
			</xpath>
            
            <xpath expr="//group[@id='header_right_group']" position="inside">
				<field name="f_note" />
			</xpath>
            
             <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='aml_tab']/field[@name='line_ids']/tree[1]/field[@name='partner_id']" position="after">
   <field name="f_partner_ref" />
  </xpath>
            
            
            
            
		</field>
	</record>




	<record id="f_invoice_tree_view_inherit_note" model="ir.ui.view">
		<field name="name">f.invoice.tree.view.inherit.note</field>

		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_out_invoice_tree" />
		<field name="arch" type="xml">
			<data>
				<xpath expr="//field[@name='state']" position="after">
					<field name="f_note" optional="show" />
				</xpath>
			</data>
		</field>
	</record>



	<record id="f_account_move_tree_view_inherit_note" model="ir.ui.view">
		<field name="name">f.account.move.tree.view.inherit.note</field>

		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_move_tree" />
		<field name="arch" type="xml">
			<data>
				<xpath expr="//field[@name='state']" position="after">
					<field name="f_note" optional="show" />
				</xpath>
			</data>
		</field>


	</record>


	<record id="f_view_account_move_filter_inherit" model="ir.ui.view">
		<field name="name">f_account_move_search_inherit</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_account_move_filter" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='name']" position="after">
				<field name="f_note" string="Note" filter_domain="[('f_note', 'ilike', self)]" />
			</xpath>
		</field>
	</record>





    
    <record id="f_view_account_noteinovicesfilter_inherit" model="ir.ui.view">
		<field name="name">f_account_invoicenote_search_inherit</field>
		<field name="model">account.move</field>
		<field name="inherit_id" ref="account.view_account_invoice_filter" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='name']" position="after">
				<field name="f_note" string="Note" filter_domain="[('f_note', 'ilike', self)]" />
			</xpath>
		</field>
	</record>


</odoo>
