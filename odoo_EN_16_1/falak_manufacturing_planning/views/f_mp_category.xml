<odoo>

	<record id="f_mp_categ_tree" model="ir.ui.view">
		<field name="name">f.mp.categ.tree</field>
		<field name="model">f.mp.categ</field>
		<field eval="8" name="priority" />
		<field name="arch" type="xml">
			<tree string="MP Category" editable="bottom">
				<field name="f_categ_name"/>
			</tree>
		</field>
	</record>


	<record id="f_action_mp_category" model="ir.actions.act_window">
		<field name="name">MP Category</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">f.mp.categ</field>
		<field name="view_mode">tree</field>
		<field name="target">current</field>
	</record>

	<menuitem id="f_menu_mp_categ" name="MP Category"
		action="f_action_mp_category" parent="mrp.menu_mrp_configuration" />

</odoo>