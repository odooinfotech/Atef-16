<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="f_product_pricing_sequence" model="ir.sequence">
            <field name="name">Product Pricing</field>
            <field name="code">f.product.pricing</field>
            <field name="prefix">PP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="f_archive_product_pricing_server_action" model="ir.actions.server">
            <field name="name">Archive</field>
            <field name="model_id" ref="f_sales_price_equation.model_f_product_pricing"/>
            <field name="binding_model_id" ref="f_sales_price_equation.model_f_product_pricing"/>
            <field name="binding_view_types">form</field>
            <field name="state">code</field>
            <field name="code">action = records.archive()</field>
        </record>
    </data>
</odoo>