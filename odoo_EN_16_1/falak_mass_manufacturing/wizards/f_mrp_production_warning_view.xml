<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="f_mrp_production_warning_form" model="ir.ui.view">
            <field name="name">f.mrp.production.warning.form</field>
            <field name="model">f.mrp.production.warning.wizard</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Warning">
                    <group>
                        <field name="f_warning_Message"/>
                        <field name="f_mass_man_id" invisible='1'/>
                    </group>
                    <footer>
                        <button name="f_mrp_orders" string="Ok" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>