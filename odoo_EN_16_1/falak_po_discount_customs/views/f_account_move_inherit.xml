<odoo>
	<data>
		<record id="f_account_move_form_inherit" model="ir.ui.view">
			<field name="name">f.account.move.form.inherit</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_unit']" position="after">
					<field name="f_po_line_full_price" optional="show" />
					<field name="f_po_line_discount" optional="show" />
				</xpath>
			</field>
		</record>
	</data>
</odoo>