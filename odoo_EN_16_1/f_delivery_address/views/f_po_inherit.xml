<odoo>

	<record id="f_po_form_vew_inherit" model="ir.ui.view"
		context="{'studio': True}">
		<field name="active" eval="True" />
		<field name="arch" type="xml">
			<data>
				<xpath expr="//field[@name='partner_id']"
					position="attributes">
					<attribute name="domain">['|','&amp;','|',("company_id","=",False),("company_id","=",company_id),("parent_id","=",partner_id),("id","=",partner_id)]
					</attribute>
				</xpath>
			</data>
		</field>
		<field name="field_parent" eval="False" />
		<field name="groups_id" eval="[(6, 0, [])]" />
		<field name="inherit_id" ref="purchase.purchase_order_form" />
		<field name="key" eval="False" />
		<field name="mode">extension</field>
		<field name="model">purchase.order</field>
		<field name="name">f.po.form.vew.inherit</field>
		<field name="priority">99</field>
		<field name="type">form</field>
	</record>
</odoo>
