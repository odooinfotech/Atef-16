<odoo>
	<record id="faccount_incoterms_tree" model="ir.ui.view">
		<field name="name">f.account.incoterms.tree</field>
		<field name="model">account.incoterms</field>
		<field name="inherit_id" ref="account.view_incoterms_tree" />
		<field name="arch" type="xml">
			<xpath expr="//field[@name='name']" position="after">
				<field name="f_incoterm_location_type" />
			</xpath>
		</field>
	</record>
</odoo>