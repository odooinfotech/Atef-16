<odoo>
    <data>
    <record id="f_inherit_product_barcode_label" model="ir.ui.view">
        <field name="name">f.barcode.label</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view" />
        <field name="arch" type="xml">
			<xpath expr="//tree//header//button[@name='action_open_label_layout']" position="replace">
			 <button string="Print Custom Labels" type="action" name="%(products_barcode_label.action_barcode_wizard_label)d"/>
			
			</xpath>             

        </field>
    </record>
    
    
     <record id="f_inherit_product_barcode_label_form" model="ir.ui.view">
        <field name="name">f.barcode.label.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view" />
        <field name="arch" type="xml">
			<xpath expr="//form//header//button[@name='action_open_label_layout']" position="replace">
			 <button string="Print Custom Labels" type="action" name="%(products_barcode_label.action_barcode_wizard_label)d"/>
			
			</xpath>             

        </field>
    </record>
    
    
    </data>
</odoo>
